{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include":"./src/",
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "template": "./node_modules/clean-jsdoc-theme",  
        "encoding": "utf8",                
        "destination": "./docs/",           
        "recurse": true
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}